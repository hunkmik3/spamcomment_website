{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "$PYTHONPATH:.",
    "FLASK_ENV": "production",
    "PYTHON_VERSION": "3.9"
  }
}